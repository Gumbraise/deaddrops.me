{% extends 'base.html.twig' %}

{% block title %}Deaddrop{% endblock %}

{% block body %}
    <h1>Deaddrop</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ deaddrop.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ deaddrop.name }}</td>
            </tr>
            <tr>
                <th>Size</th>
                <td>{{ deaddrop.size }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ deaddrop.createdAt ? deaddrop.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>About</th>
                <td>{{ deaddrop.about }}</td>
            </tr>
            <tr>
                <th>Longitude</th>
                <td>{{ deaddrop.longitude }}</td>
            </tr>
            <tr>
                <th>Latitude</th>
                <td>{{ deaddrop.latitude }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ deaddrop.status }}</td>
            </tr>
            <tr>
                <th>IsExternalReferrer</th>
                <td>{{ deaddrop.isExternalReferrer ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ deaddrop.updatedAt ? deaddrop.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Address</th>
                <td>{{ deaddrop.address }}</td>
            </tr>
            <tr>
                <th>City</th>
                <td>{{ deaddrop.city }}</td>
            </tr>
            <tr>
                <th>Country</th>
                <td>{{ deaddrop.country }}</td>
            </tr>
            <tr>
                <th>DeaddropId</th>
                <td>{{ deaddrop.deaddropId }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_deaddrop_index') }}">back to list</a>

    <a href="{{ path('app_deaddrop_edit', {'id': deaddrop.id}) }}">edit</a>

    {{ include('deaddrop/_delete_form.html.twig') }}
{% endblock %}
